+    char** kinds = g_new0(char*, 4);
+    kinds[0] = g_strdup(G_VOLUME_IDENTIFIER_KIND_LABEL);
+    kinds[1] = g_strdup(G_VOLUME_IDENTIFIER_KIND_UNIX_DEVICE);
+    kinds[2] = g_strdup(G_VOLUME_IDENTIFIER_KIND_UUID);
+    kinds[3] = NULL;
