+
+    g_return_if_fail(object != NULL);
+    g_return_if_fail(G_IS_UDISKS_DRIVE(object));
+
+    self = G_UDISKS_DRIVE(object);
+    if(self->dev)
+        g_object_unref(self->dev);
+
+    G_OBJECT_CLASS(g_udisks_drive_parent_class)->finalize(object);
+}
