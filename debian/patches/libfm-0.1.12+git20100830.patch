+    GUDisksDrive* drv = G_UDISKS_DRIVE(base);
+    GList* vols = g_volume_monitor_get_volumes(G_VOLUME_MONITOR(drv->mon));
+    GList* l;
+    GList* ret = NULL;
+    for(l = vols;l;l=l->next)
+    {
+        GUDisksVolume* vol = G_UDISKS_VOLUME(l->data);
+        if(vol->drive == drv)
+            ret = g_list_prepend(ret, vol);
+        else
