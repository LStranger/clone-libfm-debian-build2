+GList* get_connected_drives(GVolumeMonitor *mon)
+{
+    GUDisksVolumeMonitor* umon = G_UDISKS_VOLUME_MONITOR(mon);
+    GList* drvs = g_list_copy(umon->drives);
+    g_list_foreach(drvs, (GFunc)g_object_ref, NULL);
